#!/usr/bin/env bash
titles="Attitude Out Gay OUT GT gay"
#titles="Nikon Photo Camera"
for i in {0..4000..40}; do
    url="http://forum.mobilism.org/viewforum.php?f=123&start=$i"
    e="$(curl -s "$url")"
    echo "page : $i"
    for t in $titles; do
        echo "$e" | grep "$t" | grep 'viewtopic.php'
    done
done
